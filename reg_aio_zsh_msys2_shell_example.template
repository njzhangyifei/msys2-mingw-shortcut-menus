[HKEY_CLASSES_ROOT\Directory\Background\shell\msys2]
@="[&M]SYS2 Zsh Here"
"Icon"="\"{MSYS2_INSTALL_DIR}msys2.ico\""

[HKEY_CLASSES_ROOT\Directory\Background\shell\msys2\command]
